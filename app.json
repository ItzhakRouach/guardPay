{
  "expo": {
    "name": "GuardPay",
    "slug": "GuardPay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/GuardAppIcon.png",
    "scheme": "guardpay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "supportsRTL": true
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.GuardPay",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.GuardPay",
      "supportsRTL": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon-96x96.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
